{
  "kind": "collectionType",
  "collectionName": "clients",
  "info": {
    "singularName": "client",
    "pluralName": "clients",
    "displayName": "Client"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "maxLength": 20,
      "required": true,
      "unique": false
    },
    "email": {
      "type": "email",
      "required": true
    },
    "phone": {
      "type": "string"
    },
    "cpf_or_cnpj": {
      "type": "string",
      "required": true
    },
    "enterprise": {
      "type": "string"
    },
    "quotes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::quote.quote",
      "mappedBy": "client"
    }
  }
}
